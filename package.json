{
  "name": "markdown-extended",
  "displayName": "Markdown Extended",
  "description": "Extended syntaxes to built-in markdown & What you see is what you get exporter.",
  "icon": "images/logo.png",
  "version": "0.4.1",
  "publisher": "jebbs",
  "engines": {
    "vscode": "^1.15.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/qjebbs/vscode-markdown-extended/issues"
  },
  "homepage": "https://github.com/qjebbs/vscode-markdown-extended/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/qjebbs/vscode-markdown-extended.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:markdownExtended.export",
    "onCommand:markdownExtended.copy",
    "onCommand:markdownExtended.copyWithStyle",
    "onCommand:markdownExtended.pasteAsTable",
    "onCommand:markdownExtended.formateTable"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "markdownExtended.export",
        "title": "%markdownExtended.export.current%",
        "category": "Markdown"
      },
      {
        "command": "markdownExtended.copy",
        "title": "%markdownExtended.copy%",
        "category": "Markdown"
      },
      {
        "command": "markdownExtended.copyWithStyle",
        "title": "%markdownExtended.copyWithStyle%",
        "category": "Markdown"
      },
      {
        "command": "markdownExtended.pasteAsTable",
        "title": "%markdownExtended.pasteAsTable%",
        "category": "Markdown"
      },
      {
        "command": "markdownExtended.formateTable",
        "title": "%markdownExtended.formateTable%",
        "category": "Markdown"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == markdown",
          "command": "markdownExtended.export",
          "group": "markdown"
        },
        {
          "when": "editorLangId == markdown",
          "command": "markdownExtended.copy",
          "group": "markdown"
        },
        {
          "when": "editorLangId == markdown",
          "command": "markdownExtended.copyWithStyle",
          "group": "markdown"
        },
        {
          "when": "editorLangId == markdown",
          "command": "markdownExtended.pasteAsTable",
          "group": "markdown"
        },
        {
          "when": "editorLangId == markdown",
          "command": "markdownExtended.formateTable",
          "group": "markdown"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Markdown Extended Configuration",
      "properties": {
        "markdownExtended.phantomPath": {
          "type": "string",
          "default": "",
          "description": "%markdownExtended.configuration.phantomPath%"
        }
      }
    },
    "markdown.markdownItPlugins": true,
    "markdown.previewStyles": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/markdown-it": "0.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "markdown-it": "^8.3.2",
    "mocha": "^3.5.0",
    "typescript": "^2.5.2",
    "vscode": "^1.1.5"
  },
  "dependencies": {
    "@types/html-pdf": "^2.1.2",
    "clipboardy": "^1.2.3",
    "highlight.js": "^9.12.0",
    "html-pdf": "^2.1.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-attrs": "^1.1.0",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-kbd": "^1.1.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.3.2",
    "markdown-it-underline": "^1.0.1",
    "node-yaml": "^3.1.1",
    "papaparse": "^4.3.7"
  }
}
